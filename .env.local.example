# Local Development Environment Variables
# Copy this to .env and add your actual API keys

# Database (Docker Compose default)
DATABASE_URL=postgresql://postgres:postgres@db:5432/college_community

# Security
SECRET_KEY=dev-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# OpenAI API Key - Add your NEW API key here after getting it from OpenAI
# Visit: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Development Settings
ENVIRONMENT=development
DEBUG=True